{
	"routes": [ 

		{
			"source": "^/ui/(.*)$",
			"destination": "cct-ui",
			"authenticationType":"xsuaa",
			"target": "/$1"
		},
		{
			"source": "^/ts/(.*)$",
			"destination": "cct-ts-destination",
			"authenticationType":"xsuaa",
			"target": "/$1"
		},
{
			"source": "^/tsaccess/(.*)$",
			"destination": "CoDeX-destination",
			"authenticationType":"none",
			"target": "/$1"
		}
	]
}
